{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build active file",
            "command": "C:\\msys64\\ucrt64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "${workspaceFolder}\\utility\\**",
                "${workspaceFolder}\\tmds_encoder\\**",
                "${workspaceFolder}\\sim_tmds_encoder\\**",
                "-I${workspaceFolder}\\utility",
                "-I${workspaceFolder}\\tmds_encoder",
                "-I${workspaceFolder}\\sim_tmds_encoder",
                "-Wall",
                "-o",                
                "${workspaceFolder}\\output\\${fileBasenameNoExtension}.exe"
